
{
    "version": 2,
    "builds": [
      {
        "src": "frontend/ecommerce/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/ecommerce/dist",
          "installCommand": "cd frontend/ecommerce && npm install",
          "buildCommand": "cd frontend/ecommerce && npx vite build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }